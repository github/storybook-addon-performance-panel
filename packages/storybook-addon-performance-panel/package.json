{
  "name": "@github-ui/storybook-addon-performance-panel",
  "description": "A Storybook addon that provides a real-time performance monitoring panel with frame timing, input latency, layout shift, and React render profiling.",
  "type": "module",
  "version": "1.1.0",
  "license": "MIT",
  "author": "GitHub, Inc.",
  "homepage": "https://github.com/github/storybook-addon-performance-panel#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github/storybook-addon-performance-panel.git",
    "directory": "packages/storybook-addon-performance-panel"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "storybook",
    "storybook-addon",
    "performance",
    "profiling",
    "react",
    "web-vitals",
    "frame-timing",
    "layout-shift"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./universal": {
      "types": "./dist/index-universal.d.ts",
      "default": "./dist/index-universal.js"
    },
    "./universal/preset": {
      "types": "./dist/preset-universal.d.ts",
      "default": "./dist/preset-universal.js"
    },
    "./universal/preview": {
      "types": "./dist/preview-universal.d.ts",
      "default": "./dist/preview-universal.js"
    },
    "./components": {
      "types": "./dist/index-react.d.ts",
      "default": "./dist/index-react.js"
    },
    "./manager": {
      "types": "./dist/manager.d.ts",
      "default": "./dist/manager.js"
    },
    "./package.json": "./package.json",
    "./preset": {
      "types": "./dist/preset.d.ts",
      "default": "./dist/preset.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "**/*.md"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "test": "vitest run"
  },
  "bundler": {
    "managerEntries": [
      "manager.tsx"
    ],
    "previewEntries": [
      "preview.ts",
      "preview-universal.ts",
      "index.ts",
      "index-universal.ts",
      "index-react.ts"
    ],
    "nodeEntries": [
      "preset.ts",
      "preset-universal.ts"
    ]
  },
  "peerDependencies": {
    "@storybook/icons": "^2.0.0",
    "@storybook/react": "^10.0.0",
    "react": "^19",
    "storybook": "^10"
  },
  "peerDependenciesMeta": {
    "@storybook/react": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@types/node": "^25",
    "@types/react": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/browser-playwright": "^4.0.18",
    "esbuild": "0.27.3",
    "publint": "^0.3.17",
    "tsdown": "^0.20.3",
    "vitest": "^4.0.18",
    "vitest-browser-react": "^2.0.5"
  },
  "storybook": {
    "displayName": "Addon Performance Panel",
    "supportedFrameworks": [
      "react",
      "vue",
      "svelte",
      "angular",
      "lit",
      "preact",
      "web-components",
      "html"
    ],
    "icon": "https://github.githubassets.com/favicons/favicon.svg"
  }
}
