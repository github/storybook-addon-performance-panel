{
  "name": "@github-ui/storybook-addon-performance-panel",
  "description": "A Storybook addon that provides a real-time performance monitoring panel with frame timing, input latency, layout shift, and React render profiling.",
  "type": "module",
  "version": "1.0.0",
  "license": "MIT",
  "author": "GitHub, Inc.",
  "homepage": "https://github.com/github/storybook-addon-performance-panel#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/github/storybook-addon-performance-panel.git",
    "directory": "packages/storybook-addon-performance-panel"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "storybook",
    "storybook-addon",
    "performance",
    "profiling",
    "react",
    "web-vitals",
    "frame-timing",
    "layout-shift"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./manager": {
      "types": "./dist/manager.d.ts",
      "default": "./dist/manager.js"
    },
    "./package.json": "./package.json",
    "./preset": {
      "types": "./dist/preset.d.ts",
      "default": "./dist/preset.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "**/*.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "vitest run"
  },
  "bundler": {
    "managerEntries": [
      "manager.tsx"
    ],
    "previewEntries": [
      "preview.ts",
      "index.ts"
    ],
    "nodeEntries": [
      "preset.ts"
    ]
  },
  "peerDependencies": {
    "@storybook/icons": "^2.0.0",
    "@storybook/react": "^10.0.0",
    "react": "^19",
    "storybook": "^10"
  },
  "devDependencies": {
    "@types/node": "^25",
    "@types/react": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/browser-playwright": "^4.0.18",
    "esbuild": "0.27.3",
    "tsup": "^8.5.1",
    "vitest": "^4.0.18",
    "vitest-browser-react": "^2.0.5"
  },
  "storybook": {
    "displayName": "Addon Performance Panel",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://github.githubassets.com/favicons/favicon.svg"
  }
}
