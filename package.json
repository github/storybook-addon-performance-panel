{
  "name": "storybook-addon-performance-panel-repo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build -w packages/storybook-addon-performance-panel",
    "build:watch": "npm run build:watch -w packages/storybook-addon-performance-panel",
    "lint": "eslint . --cache",
    "test": "npm test -w packages/storybook-addon-performance-panel",
    "tsc": "tsc -b packages/storybook-addon-performance-panel/tsconfig.build.json",
    "demo": "npm run build && npm run storybook -w packages/demo",
    "demo:build": "npm run build && npm run build-storybook -w packages/demo"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
